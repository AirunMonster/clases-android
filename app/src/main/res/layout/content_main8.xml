<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main8"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="1"
    android:stretchColumns="*"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="holamundo.app.morales.nuria.holamundo.Main8Activity"
    tools:showIn="@layout/activity_main8">

    <TextView
        android:text="Ejemplo de Dos Fragments en una Activity"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="0.45"/>

    <fragment
        android:id="@+id/fragment_Historial"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="0.28"
        class="holamundo.app.morales.nuria.holamundo.FragmentHistorial">
    </fragment>

    <fragment
        android:id="@+id/fragment_HistorialB"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="0.27"
        class="holamundo.app.morales.nuria.holamundo.FragmentHistorialB">
    </fragment>

</LinearLayout>
